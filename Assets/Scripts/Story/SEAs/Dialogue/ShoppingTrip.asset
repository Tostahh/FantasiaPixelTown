  m_Script: {fileID: 11500000, guid: f445001872a0b1a45aa598412e3bdcc7, type: 3}
  m_Name: ShoppingTrip
  m_EditorClassIdentifier: Assembly-CSharp::DialogueSO
  dialogueID: ShoppingTrip
  lines:
  - ImageSprite: {fileID: 7253254653333588038, guid: cee6599f40a14854a8a35277cbc0e844, type: 3}
    speakerName: Elara
    text: "It\u2019s\u2026 wonderful to see the square alive again."
  - ImageSprite: {fileID: 21300000, guid: a6ac6f835b210334aa91ed6b950f354f, type: 3}
    speakerName: Hue
    text: You really think the town can feel safe now?
  - ImageSprite: {fileID: 7253254653333588038, guid: cee6599f40a14854a8a35277cbc0e844, type: 3}
    speakerName: Elara
    text: "With the guard post up? Yes! Everyone\u2019s starting to breathe again.
      It\u2019s like the mist never touched us."
  - ImageSprite: {fileID: 7253254653333588038, guid: cee6599f40a14854a8a35277cbc0e844, type: 3}
    speakerName: Elara
    text: "I\u2019ve been thinking\u2026 a Market Square. A place for trade, for
      laughter, for life. Don\u2019t you think it\u2019s time?"
  - ImageSprite: {fileID: 21300000, guid: a6ac6f835b210334aa91ed6b950f354f, type: 3}
    speakerName: Hue
    text: "Then let\u2019s make it happen."
