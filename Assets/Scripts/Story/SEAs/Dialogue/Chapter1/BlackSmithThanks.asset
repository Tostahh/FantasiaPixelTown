  dialogueID: BlackSmithThanks
  lines:
  - ImageSprite: {fileID: 21300000, guid: 0365174485713ee4a9d35df90587e39e, type: 3}
    speakerName: Bagren
    text: "\u2026Oh\u2026 someone\u2019s here."
  - ImageSprite: {fileID: 21300000, guid: a6ac6f835b210334aa91ed6b950f354f, type: 3}
    speakerName: Hue
    text: "You\u2019ve been here this whole time?"
  - ImageSprite: {fileID: 21300000, guid: 0365174485713ee4a9d35df90587e39e, type: 3}
    speakerName: Bargen
    text: "Y-yes\u2026 I keep to myself."
  - ImageSprite: {fileID: 21300000, guid: a6ac6f835b210334aa91ed6b950f354f, type: 3}
    speakerName: Hue
    text: "You\u2019re the town\u2019s blacksmith, right?"
  - ImageSprite: {fileID: 21300000, guid: 0365174485713ee4a9d35df90587e39e, type: 3}
    speakerName: Bargen
    text: "I\u2026 I am. Please, don\u2019t look under my... bag."
  - ImageSprite: {fileID: 21300000, guid: a6ac6f835b210334aa91ed6b950f354f, type: 3}
    speakerName: Hue
    text: "So you\u2019ll help rebuild?"
  - ImageSprite: {fileID: 21300000, guid: 0365174485713ee4a9d35df90587e39e, type: 3}
    speakerName: Bargen
    text: "Y-yes\u2026 I\u2019ll do my best\u2026 heh."
